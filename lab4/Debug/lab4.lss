                                 ;.include "LCDDriver.asm"		; Include LCD driver
                                 ;; We are not allowed to include "LCDDriver.asm" here because
                                 ;; this is a purely pre-compiler directives. This contains
                                 ;; actual assembly instructins and thus cannot be inlucded
                                 ;; at the beginning of our main program file.
                                 ;; As indicated in the AVR Starter Guide, any included code
                                 ;; files are cindlued at the end of the main program
                                 ;; i.e. the last line(s)
                                 
                                 ;***********************************************************
                                 ;*	Internal Register Definitions and Constants
                                 ;***********************************************************
                                 .def	mpr = r16				; Multipurpose register is
                                 								; required for LCD Driver
                                 
                                 ;***********************************************************
                                 ;*	Start of Code Segment
                                 ;***********************************************************
                                 .cseg							; Beginning of code segment
                                 
                                 ;***********************************************************
                                 ;*	Interrupt Vectors
                                 ;***********************************************************
                                 .org	$0000					; Beginning of IVs
000000 c045                      		rjmp INIT				; Reset interrupt
                                 
                                 .org	$0046					; End of Interrupt Vectors
                                 
                                 ;***********************************************************
                                 ;*	Program Initialization
                                 ;***********************************************************
                                 INIT:							
                                 		; Initialize Stack Pointer
                                 		; Point SP to the end of RAM (RAMEND)
000046 ef0f                      		ldi		mpr, low(RAMEND)
000047 bf0d                      		out		SPL, mpr			
000048 e100                      		ldi		mpr, high(RAMEND)
000049 bf0e                      		out		SPH, mpr
                                 		;; NOTE
                                 		;; To be able to call any function (subroutine),
                                 		;; we must initialize the Stack Pointer 
                                 		;; at very first
                                 
                                 		;; Initialize LCD Display
00004a d037                      		rcall	LCDInit			; init LCD
                                 		;rcall	LCDClear		; Clear both lines of the LCD
                                 
                                 		;; Move strings from Program Memory to Data Memory
00004b eee4                      		ldi		ZL, low(STRING_BEG << 1)
00004c e0f0                      		ldi		ZH, high(STRING_BEG << 1)
00004d e0c4                      		ldi		YL, low(STRING_END << 1)
00004e e0d1                      		ldi		YH, high(STRING_END << 1)
                                 		;; Point X-register to the appropriate mem location
00004f e0b1                      		ldi		XH, $01
000050 e0a0                      		ldi		XL, $00
000051 e090                      		ldi		r25, 0
                                 
                                 		;; X, Y,Z register
                                 		;; X is for displaying
                                 		;; Z is for holding STRING_BEG
                                 		;; Y is for holding STRING_END
                                 
                                 		;; Set up PORTB for non-device test
000052 ef0f                      		ldi		mpr, $ff
000053 bb08                      		out		PORTB, mpr		; all pins at port B is 1
000054 bb07                      		out		DDRB, mpr		; set port B as output (for testing our string)
                                 
                                 		; NOTE that there is no RET or RJMP from INIT, this
                                 		; is because the next instruction executed is the
                                 		; first instruction of the MAIN program
                                 
                                 ;***********************************************************
                                 ;*	Loop
                                 ;***********************************************************
                                 LOOP:
                                 		
                                 		
                                 
                                 		;; Load byte into mpr
000055 9105                      		lpm		mpr, Z+			; Load byte from Program Mem into mpr
                                 		
                                 
                                 		;; Display
000056 930d                      		st		X+, mpr
000057 d069                      		rcall	LCDWrite		; Write to both lines of the LCD
                                 		;out	PORTB, mpr		; Display the byte we just read onto the LCD Display
                                 
000058 17ec                      		cp		ZL, YL			; Check if we have reached the end of string
000059 f091                      		breq	DONE1			; End of program - scroll now
                                 
                                 		;; Move the 
                                 		;st		Y+, mpr			; DataMemory[Y] <- mpr, Y++
                                 
00005a cffa                      		rjmp	LOOP			; jump back to main and create an infinite
                                 								; while loop.  Generally, every main program is an
                                 								; infinite while loop, never let the main program
                                 								; just run off
                                 
                                 SCROLL:
00005b e0b1                      		ldi		XH, $01			; X register holds first line pointer
00005c e0a0                      		ldi		XL, $00
00005d 918d                      		ld		r24, X+			; store new value in r24
                                 SCROLLLOOP:
00005e 2f78                      		mov		r23, r24		; copy r24 to r23
00005f 918c                      		ld		r24, X			; now new value is in r23, old value in r24
000060 937c                      		st		X, r23			; store new value in X
000061 9611                      		adiw	X, 1
000062 30b1                      		cpi		XH, $01
000063 f009                      		breq	SECONDCHECK
000064 cff9                      		rjmp	SCROLLLOOP
                                 
                                 SECONDCHECK:
000065 32a0                      		cpi		XL, $20			; we've reached the end!
000066 f009                      		breq	LASTCHAR
000067 cff6                      		rjmp	SCROLLLOOP
                                 
                                 LASTCHAR:
000068 e0b1                      		ldi		XH, $01			; X register holds first line pointer
000069 e0a0                      		ldi		XL, $00
00006a 938c                      		st		X, r24
00006b cfef                      		rjmp	SCROLL
                                 		
                                 DONE1:	
00006c 17fd                      		cp		ZH, YH			; Check if we have reached the end of string
00006d f369                      		breq	SCROLL			; TODO: When will this happen?
00006e cfe6                      		rjmp	LOOP			; Loop infinte
                                 
                                 ;***********************************************************
                                 ;*	Main Program
                                 ;***********************************************************
                                 MAIN:							; The Main program
                                 		
00006f cfe5                      		rjmp	LOOP
000070 cffe                      		rjmp	MAIN			; jump back to main and create an infinite
                                 								; while loop.  Generally, every main program is an
                                 								; infinite while loop, never let the main program
                                 								; just run off
                                 
