                                 .include "LCDDriver.asm"		; Include the LCD Driver
                                 
                                 ;*
                                 ;*	LCDDriver.asm	-	V2.0
                                 ;*
                                 ;*	Contains the neccessary functions to display text to a
                                 ;*	2 x 16 character LCD Display.  Additional functions
                                 ;*	include a conversion routine from an unsigned 8-bit
                                 ;*	binary number to and ASCII text string.
                                 ;*
                                 ;*	Version 2.0 - Added support for accessing the LCD 
                                 ;*		Display via the serial port. See version 1.0 for 
                                 ;*		accessing a memory mapped LCD display.
                                 ;*
                                 ;***********************************************************
                                 ;*
                                 ;*	 Author: David Zier
                                 ;*	   Date: March 17, 2003
                                 ;*	Company: TekBots(TM), Oregon State University - EECS
                                 ;*	Version: 2.0
                                 ;*
                                 ;***********************************************************
                                 ;*	Rev	Date	Name		Description
                                 ;*----------------------------------------------------------
                                 ;*	-	8/20/02	Zier		Initial Creation of Version 1.0
                                 ;*	A	3/7/03	Zier		V2.0 - Updated for USART LCD
                                 ;*
                                 ;*
                                 ;***********************************************************
                                 
                                 ;***********************************************************
                                 ;*	Internal Register Definitions and Constants
                                 ;*		NOTE: A register MUST be named 'mpr' in the Main Code
                                 ;*			It is recomended to use register r16.
                                 ;*		WARNING: Register r17-r22 are reserved and cannot be
                                 ;*			renamed outside of the LCD Driver functions. Doing
                                 ;*			so will damage the functionality of the LCD Driver
                                 ;***********************************************************
                                 .def	wait = r17				; Wait Loop Register
                                 .def	count = r18				; Character Counter
                                 .def	line = r19				; Line Select Register
                                 .def	type = r20				; LCD data type: Command or Text
                                 .def	q = r21					; Quotient for div10
                                 .def	r = r22					; Remander for div10
                                 
                                 .equ	LCDLine1 = $80			; LCD Line 1 select command
                                 .equ	LCDLine2 = $c0			; LCD Line 2 select command
                                 .equ	LCDClear = $01			; LCD Clear Command
                                 .equ	LCDHome = $02			; LCD Set Cursor Home Command
                                 .equ	LCDPulse = $08			; LCD Pulse signal, used to simulate 
                                 								; write signal
                                 
                                 .equ	LCDCmd = $00			; Constant used to write a command 
                                 .equ	LCDTxt = $01			; Constant used to write a text character
                                 
                                 .equ	LCDMaxCnt = 16			; Maximum number of characters per line
                                 .equ	LCDLn1Addr = $0100		; Beginning address for Line 1 data
                                 .equ	LCDLn2Addr = $0110		; Beginning address for Line 2 data
                                 
                                 ;-----------------------------------------------------------
                                 ;***********************************************************
                                 ;*	Public LCD Driver Suboutines and Functions
                                 ;*		These functions and subroutines can be called safely 
                                 ;*		from within any program
                                 ;***********************************************************
                                 ;-----------------------------------------------------------
                                 
                                 
                                 ;*******************************************************
                                 ;* SubRt: 	LCDInit
                                 ;* Desc: 	Initialize the Serial Port and the Hitachi 
                                 ;*			Display 8 Bit inc DD-RAM 
                                 ;*			Pointer with no features
                                 ;*			- 2 LInes with 16 characters
                                 ;*******************************************************
                                 LCDInit:
000082 930f                      		push	mpr				; Save the state of machine
000083 b70f                      		in		mpr, SREG		; Save the SREG
000084 930f                      		push	mpr				;
000085 931f                      		push	wait			; Save wait
                                 
                                 		; Setup the Communication Ports
                                 		; Port B: Output
                                 		; Port D: Input w/ internal pullup resistors
                                 		; Port F: Output on Pin 3
000086 e000                      		ldi		mpr, $00		; Initialize Port B for outputs
000087 bb08                      		out		PORTB, mpr		; Port B outputs high
000088 ef0f                      		ldi		mpr, $ff		; except for any overrides
000089 bb07                      		out		DDRB, mpr		;
00008a e000                      		ldi		mpr, $00		; Initialize Port D for inputs
00008b bb02                      		out		PORTD, mpr		; with Tri-State
00008c e000                      		ldi		mpr, $00		; except for any overrides
00008d bb01                      		out		DDRD, mpr		;
00008e e000                      		ldi		mpr, $00		; Initialize Port F Pin 3 to
00008f 9300 0062                 		sts		PORTF, mpr		; output inorder to twiddle the
000091 e008                      		ldi		mpr, (1<<DDF3)	; LCD interface
000092 9300 0061                 		sts		DDRF, mpr		; Must NOT override this port
                                 
                                 		; Setup the Serial Functionality
                                 		; SPI Type: Master
                                 		; SPI Clock Rate: 2*1000.000 kHz
                                 		; SPI Clock Phase: Cycle Half
                                 		; SPI Clock Polarity: Low
                                 		; SPI Data Order: MSB First
000094 e500                      		ldi		mpr, (1<<SPE|1<<MSTR)
000095 b90d                      		out		SPCR, mpr		; Set Serial Port Control Register
000096 e001                      		ldi		mpr, (1<<SPI2X)
000097 b90e                      		out		SPSR, mpr		; Set Serial Port Status Register
                                 
                                 		; Setup External SRAM configuration
                                 		; $0460 - $7FFF / $8000 - $FFFF
                                 		; Lower page wait state(s): None
                                 		; Uppoer page wait state(s): 2r/w
000098 e800                      		ldi		mpr, (1<<SRE)	; 
000099 bf05                      		out		MCUCR, mpr		; Initialize MCUCR
00009a e402                      		ldi		mpr, (1<<SRL2|1<<SRW11)
00009b 9300 006d                 		sts		XMCRA, mpr		; Initialize XMCRA
00009d e800                      		ldi		mpr, (1<<XMBK)	;
00009e 9300 006c                 		sts		XMCRB, mpr		; Initialize XMCRB
                                 		
                                 		; Initialize USART0
                                 		; Communication Parameter: 8 bit, 1 stop, No Parity
                                 		; USART0 Rx: On
                                 		; USART0 Tx: On
                                 		; USART0 Mode: Asynchronous
                                 		; USART0 Baudrate: 9600
0000a0 e000                      		ldi		mpr, $00		;
0000a1 b90b                      		out		UCSR0A, mpr		; Init UCSR0A
0000a2 e108                      		ldi		mpr, (1<<RXEN0|1<<TXEN0)
0000a3 b90a                      		out		UCSR0B, mpr		; Init UCSR0B
0000a4 e006                      		ldi		mpr, (1<<UCSZ01|1<<UCSZ00)
0000a5 9300 0095                 		sts		UCSR0C, mpr		; Init UCSR0C
0000a7 e000                      		ldi		mpr, $00		;
0000a8 9300 0090                 		sts		UBRR0H, mpr		; Init UBRR0H
0000aa e607                      		ldi		mpr, $67		;
0000ab b909                      		out		UBRR0L, mpr		; Init UBRR0L
                                 
                                 		; Initialize the LCD Display
0000ac e006                      		ldi		mpr, 6			;
                                 LCDINIT_L1:
0000ad ef1a                      		ldi		wait, 250		; 15ms of Display
0000ae d0bf                      		rcall	LCDWait			; Bootup wait
0000af 950a                      		dec		mpr				;
0000b0 f7e1                      		brne	LCDINIT_L1		;
                                 
0000b1 e308                      		ldi		mpr, $38		; Display Mode set
0000b2 d097                      		rcall 	LCDWriteCmd		; 
0000b3 e008                      		ldi		mpr, $08		; Display Off
0000b4 d095                      		rcall	LCDWriteCmd		;
0000b5 e001                      		ldi		mpr, $01		; Display Clear
0000b6 d093                      		rcall	LCDWriteCmd		;
0000b7 e006                      		ldi		mpr, $06		; Entry mode set
0000b8 d091                      		rcall	LCDWriteCmd		;
0000b9 e00c                      		ldi		mpr, $0c		; Display on
0000ba d08f                      		rcall	LCDWriteCmd		;
0000bb d028                      		rcall	LCDClr			; Clear display
                                 
0000bc 911f                      		pop		wait			; Restore wait
0000bd 910f                      		pop		mpr				; Restore SREG
0000be bf0f                      		out		SREG, mpr		;
0000bf 910f                      		pop		mpr				; Restore mpr
0000c0 9508                      		ret						; Return from subroutine
                                 
                                 ;*******************************************************
                                 ;* Func:	LCDWrite
                                 ;* Desc:	Generic Write Function that writes both lines
                                 ;*			of text out to the LCD
                                 ;*			- Line 1 data is in address space $0100-$010F
                                 ;*			- Line 2 data is in address space $0110-$010F
                                 ;*******************************************************
                                 LCDWrite:
0000c1 d002                      		rcall LCDWrLn1			; Write Line 1
0000c2 d011                      		rcall LCDWrLn2			; Write Line 2
0000c3 9508                      		ret 					; Return from function
                                 
                                 ;*******************************************************
                                 ;* Func:	LCDWrLn1
                                 ;* Desc:	This function will write the first line of 
                                 ;*			data to the first line of the LCD Display
                                 ;*******************************************************
                                 LCDWrLn1:
0000c4 930f                      		push 	mpr				; Save mpr
0000c5 93ef                      		push	ZL				; Save Z pointer
0000c6 93ff                      		push	ZH				;
0000c7 932f                      		push	count			; Save the count register
0000c8 933f                      		push	line			; Save the line register
                                 								
0000c9 e0e0                      		ldi		ZL, low(LCDLn1Addr)
0000ca e0f1                      		ldi		ZH, high(LCDLn1Addr)		 
0000cb e830                      		ldi		line, LCDLine1	; Set LCD line to Line 1
0000cc d06b                      		rcall	LCDSetLine		; Restart at the beginning of line 1
0000cd d076                      		rcall	LCDWriteLine	; Write the line of text
                                 
0000ce 913f                      		pop		line
0000cf 912f                      		pop		count			; Restore the counter
0000d0 91ff                      		pop		ZH				; Restore Z pointer
0000d1 91ef                      		pop		ZL				;
0000d2 910f                      		pop 	mpr				; Restore mpr
0000d3 9508                      		ret						; Return from function
                                 
                                 ;*******************************************************
                                 ;* Func:	LCDWrLn2
                                 ;* Desc:	This function will write the second line of 
                                 ;*			data to the second line of the LCD Display
                                 ;*******************************************************
                                 LCDWrLn2:
0000d4 930f                      		push 	mpr				; Save mpr
0000d5 93ef                      		push	ZL				; Save Z pointer
0000d6 93ff                      		push	ZH				;
0000d7 932f                      		push	count			; Save the count register
0000d8 933f                      		push	line			; Save the line register
                                 								
0000d9 e1e0                      		ldi		ZL, low(LCDLn2Addr)
0000da e0f1                      		ldi		ZH, high(LCDLn2Addr)		 
0000db ec30                      		ldi		line, LCDLine2	; Set LCD line to Line 2
0000dc d05b                      		rcall	LCDSetLine		; Restart at the beginning of line 2
0000dd d066                      		rcall	LCDWriteLine	; Write the line of text
                                 
0000de 913f                      		pop		line
0000df 912f                      		pop		count			; Restore the counter
0000e0 91ff                      		pop		ZH				; Restore Z pointer
0000e1 91ef                      		pop		ZL				;
0000e2 910f                      		pop 	mpr				; Restore mpr
0000e3 9508                      		ret						; Return from function
                                 
                                 ;*******************************************************
                                 ;* Func:	LCDClr
                                 ;* Desc:	Generic Clear Subroutine that clears both 
                                 ;*			lines of the LCD and Data Memory storage area
                                 ;*******************************************************
                                 LCDClr:
0000e4 d002                      		rcall	LCDClrLn1		; Clear Line 1
0000e5 d011                      		rcall	LCDClrLn2		; Clear Line 2
0000e6 9508                      		ret						; Return from Subroutine
                                 
                                 ;*******************************************************
                                 ;* Func:	LCDClrLn1
                                 ;* Desc:	This subroutine will clear the first line of 
                                 ;*			the data and the first line of the LCD Display
                                 ;*******************************************************
                                 LCDClrLn1:
0000e7 930f                      		push	mpr				; Save mpr
0000e8 933f                      		push	line			; Save line register
0000e9 932f                      		push	count			; Save the count register
0000ea 93ef                      		push	ZL				; Save Z pointer
0000eb 93ff                      		push	ZH				;
                                 	
0000ec e830                      		ldi		line, LCDline1	; Set Access to Line 1 of LCD
0000ed d04a                      		rcall	LCDSetLine		; Set Z pointer to address of line 1 data
0000ee e0e0                      		ldi		ZL, low(LCDLn1Addr)
0000ef e0f1                      		ldi		ZH, high(LCDLn1Addr)
0000f0 d04c                      		rcall	LCDClrLine		; Call the Clear Line function
                                 
0000f1 91ff                      		pop		ZH				; Restore Z pointer
0000f2 91ef                      		pop		ZL				;
0000f3 912f                      		pop		count			; Restore the count register
0000f4 913f                      		pop		line			; Restore line register
0000f5 910f                      		pop		mpr				; Restore mpr
0000f6 9508                      		ret						; Return from Subroutine
                                 
                                 ;*******************************************************
                                 ;* Func:	LCDClrLn2
                                 ;* Desc:	This subroutine will clear the second line of 
                                 ;*			the data and the second line of the LCD Display
                                 ;*******************************************************
                                 LCDClrLn2:
0000f7 930f                      		push	mpr				; Save mpr
0000f8 933f                      		push	line			; Save line register
0000f9 932f                      		push	count			; Save the count register
0000fa 93ef                      		push	ZL				; Save Z pointer
0000fb 93ff                      		push	ZH				;
                                 	
0000fc ec30                      		ldi		line, LCDline2	; Set Access to Line 2 of LCD
0000fd d03a                      		rcall	LCDSetLine		; Set Z pointer to address of line 2 data
0000fe e1e0                      		ldi		ZL, low(LCDLn2Addr)
0000ff e0f1                      		ldi		ZH, high(LCDLn2Addr)
000100 d03c                      		rcall	LCDClrLine		; Call the Clear Line function
                                 
000101 91ff                      		pop		ZH				; Restore Z pointer
000102 91ef                      		pop		ZL				;
000103 912f                      		pop		count			; Restore the count register
000104 913f                      		pop		line			; Restore line register
000105 910f                      		pop		mpr				; Restore mpr
000106 9508                      		ret						; Return from Subroutine
                                 
                                 ;*******************************************************
                                 ;* Func:	LCDWriteByte
                                 ;* Desc:	This is a complex and low level function that
                                 ;*			allows any program to write any ASCII character
                                 ;*			(Byte) anywhere in the LCD Display.  There
                                 ;*			are several things that need to be initialized
                                 ;*			before this function is called:
                                 ;*		count - Holds the index value of the line to where
                                 ;*				the char is written, 0-15(39).  i.e. if 
                                 ;*				count has the value of 3, then the char is
                                 ;*				going to be written to the third element of
                                 ;*				the line.
                                 ;*		line  - Holds the line number that the char is going
                                 ;*				to be written to, (1 or 2).
                                 ;*		mpr	  - Contains the value of the ASCII character to 
                                 ;*				be written (0-255)
                                 ;*********************************************************
                                 LCDWriteByte:
000107 930f                      		push	mpr				; Save the mpr
000108 933f                      		push	line			; Save the line
000109 932f                      		push	count			; Save the count
                                 								; Preform sanity checks on count and line
00010a 3228                      		cpi		count, 40		; Make sure count is within range
00010b f450                      		brsh	LCDWriteByte_3	; Do nothing and exit function
00010c 3031                      		cpi		line, 1			; If (line == 1)
00010d f411                      		brne	LCDWriteByte_1	; 
00010e e830                      		ldi		line, LCDLine1	; Load line 1 base LCD Address
00010f c003                      		rjmp	LCDWriteByte_2	; Continue on with function
                                 LCDWriteByte_1:					
000110 3032                      		cpi		line, 2			; If (line == 2)
000111 f421                      		brne	LCDWriteByte_3	; Do nothing and exit function
000112 ec30                      		ldi		line, LCDLine2	; Load line 2 base LCD Address
                                 
                                 LCDWriteByte_2:					; Write char to LCD
000113 0f32                      		add		line, count		; Set the correct LCD address
000114 d023                      		rcall	LCDSetLine		; Set the line address to LCD
000115 d042                      		rcall	LCDWriteChar	; Write Char to LCD Display		
                                 		
                                 LCDWriteByte_3:					; Exit Function
000116 912f                      		pop		count			; Restore the count
000117 913f                      		pop		line			; Restore the line
000118 910f                      		pop		mpr				; Restore the mpr
000119 9508                      		ret						; Return from function
                                 		 
                                 ;*******************************************************
                                 ;* Func:	Bin2ASCII
                                 ;* Desc:	Converts a binary number into an ASCII 
                                 ;*			text string equivalent. 
                                 ;*			- The binary number needs to be in the mpr
                                 ;*			- The Start Address of where the text will
                                 ;*			 	be placed needs to be in the X Register
                                 ;*			- The count of the characters created are 
                                 ;*				added to the count register
                                 ;*******************************************************
                                 Bin2ASCII:
00011a 930f                      		push	mpr				; save mpr
00011b 936f                      		push	r				; save r
00011c 935f                      		push	q				; save q
00011d 93bf                      		push	XH				; save X-pointer
00011e 93af                      		push	XL				;
                                 
                                 		; Determine the range of mpr
00011f 3604                      		cpi		mpr, 100		; is mpr >= 100
000120 f018                      		brlo	B2A_1			; goto next check
000121 e023                      		ldi		count, 3		; Three chars are written
000122 9613                      		adiw	XL, 3			; Increment X 3 address spaces
000123 c007                      		rjmp	B2A_3			; Continue with program
000124 300a                      B2A_1:	cpi		mpr, 10			; is mpr >= 10
000125 f018                      		brlo	B2A_2			; Continue with program
000126 e022                      		ldi		count, 2		; Two chars are written
000127 9612                      		adiw	XL, 2			; Increment X 2 address spaces
000128 c002                      		rjmp	B2A_3			; Continue with program
000129 9611                      B2A_2:	adiw	XL, 1			; Increment X 1 address space
00012a e021                      		ldi		count, 1 		; One char is written
                                 
                                 B2A_3:	;Do-While statement that converts Binary to ASCII
00012b d04a                      		rcall	div10			; Call the div10 function
00012c e300                      		ldi		mpr, '0'		; Set the base ASCII integer value
00012d 0f06                      		add		mpr, r			; Create the ASCII integer value
00012e 930e                      		st		-X, mpr			; Load ASCII value to memory
00012f 2f05                      		mov		mpr, q			; Set mpr to quotiant value
000130 3000                      		cpi		mpr, 0			; does mpr == 0
000131 f7c9                      		brne	B2A_3			; do while (mpr != 0)
                                 
000132 91af                      		pop		XL				; restore X-pointer
000133 91bf                      		pop		XH				;
000134 915f                      		pop 	q				; restore q
000135 916f                      		pop		r				; restore r
000136 910f                      		pop		mpr				; restore mpr
000137 9508                      		ret						; return from function
                                 
                                 ;-------------------------------------------------------
                                 ;*******************************************************
                                 ;* Private LCD Driver Functions and Subroutines
                                 ;*	NOTE: It is not recommended to call these functions
                                 ;*	      or subroutines, only call the Public ones.
                                 ;*******************************************************
                                 ;-------------------------------------------------------
                                 
                                 ;*******************************************************
                                 ;* Func:	LCDSetLine
                                 ;* Desc:	Change line to be written to 
                                 ;*******************************************************
                                 LCDSetLine:
000138 930f                      		push	mpr				; Save mpr
000139 2f03                      		mov		mpr,line		; Copy Command Data to mpr
00013a d00f                      		rcall	LCDWriteCmd		; Write the Command
00013b 910f                      		pop		mpr				; Restore the mpr
00013c 9508                      		ret						; Return from function
                                 
                                 ;*******************************************************
                                 ;* Func:	LCDClrLine
                                 ;* Desc:	Manually clears a single line within an LCD
                                 ;*			Display and Data Memory by writing 16 
                                 ;*			consecutive ASCII spaces $20 to both the LCD 
                                 ;*			and the memory.  The line to be cleared must
                                 ;*			first be set in the LCD and the Z pointer is
                                 ;*			pointing the first element in Data Memory
                                 ;*******************************************************
                                 LCDClrLine:
00013d e200                      		ldi		mpr, ' '		; The space char to be written
00013e e120                      		ldi		count, LCDMaxCnt; The character count
                                 LCDClrLine_1:
00013f 9301                      		st		Z+, mpr			; Clear data memory element
000140 d017                      		rcall	LCDWriteChar	; Clear LCD memory element
000141 952a                      		dec		count			; Decrement the count
000142 f7e1                      		brne	LCDClrLine_1	; Continue untill all elements are cleared
000143 9508                      		ret						; Return from function
                                 
                                 ;*******************************************************
                                 ;* Func:	LCDWriteLine
                                 ;* Desc:	Writes a line of text to the LCD Display.
                                 ;*			This routine takes a data element pointed to
                                 ;*			by the Z-pointer and copies it to the LCD 
                                 ;*			Display for the duration of the line.  The
                                 ;*			line the Z-pointer must be set prior to the 
                                 ;*			function call.
                                 ;*******************************************************
                                 LCDWriteLine:
000144 e120                      		ldi		count, LCDMaxCnt; The character count
                                 LCDWriteLine_1:
000145 9101                      		ld		mpr, Z+			; Get the data element
000146 d011                      		rcall	LCDWriteChar	; Write element to LCD Display
000147 952a                      		dec		count			; Decrement the count
000148 f7e1                      		brne	LCDWriteLine_1	; Continue untill all elements are written
000149 9508                      		ret						; Return from function
                                 
                                 ;*******************************************************
                                 ;* Func:	LCDWriteCmd
                                 ;* Desc:	Write command that is in the mpr to LCD 
                                 ;*******************************************************
                                 LCDWriteCmd:
00014a 934f                      		push	type			; Save type register
00014b 931f                      		push	wait			; Save wait register
00014c e040                      		ldi		type, LCDCmd	; Set type to Command data
00014d d013                      		rcall	LCDWriteData	; Write data to LCD
00014e 930f                      		push	mpr				; Save mpr register
00014f e002                      		ldi		mpr, 2			; Wait approx. 4.1 ms
                                 LCDWC_L1:
000150 ec1d                      		ldi		wait, 205		; Wait 2050 us
000151 d01c                      		rcall	LCDWait			;
000152 950a                      		dec		mpr				; The wait loop cont.
000153 f7e1                      		brne	LCDWC_L1		;
000154 910f                      		pop		mpr				; Restore mpr
000155 911f                      		pop		wait			; Restore wait register
000156 914f                      		pop		type			; Restore type register
000157 9508                      		ret						; Return from function
                                 
                                 ;*******************************************************
                                 ;* Func:	LCDWriteChar
                                 ;* Desc:	Write character data that is in the mpr
                                 ;*******************************************************
                                 LCDWriteChar:
000158 934f                      		push	type			; Save type register
000159 931f                      		push	wait			; Save the wait register
00015a e041                      		ldi		type, LCDTxt	; Set type to Text data
00015b d005                      		rcall	LCDWriteData	; Write data to LCD
